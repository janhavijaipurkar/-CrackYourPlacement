class Solution {
    public int removeDuplicates(int[] arr) {
        int i=1;int j=0;
        int idx=0;
        while(i<arr.length ){
        while(i<arr.length && arr[i]==arr[j]){
            i++;
        }
        if(i<arr.length){
        idx++;
        arr[idx]=arr[i];
        j=i;
        if(i==arr.length-1){break;}
        i=i+1;
        }
        }
        return idx+1;
    }
}
